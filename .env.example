# Frontend Environment Variables
# Copy this file to .env.local for local development

# Backend API URL (for proxy mode)
VITE_API_URL=http://localhost:3001

# Direct API Keys (optional - can use proxy instead)
# Only add these if NOT using the backend proxy
# VITE_ANTHROPIC_API_KEY=
# VITE_OPENAI_API_KEY=
# VITE_GEMINI_API_KEY=

# Application Settings
VITE_APP_NAME=Discharge Summary Generator
VITE_APP_VERSION=1.0.0

# Debug Mode (true/false)
VITE_DEBUG_MODE=false

# Privacy Settings
VITE_AUTO_SAVE_ENABLED=false
VITE_TELEMETRY_ENABLED=false

# NOTE: Do NOT commit actual API keys to version control
# Keep sensitive data in .env.local which is gitignored
